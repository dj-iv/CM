{
  "version": 2,
  "functions": {
    "api/convert-to-pdf.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "PDFSHIFT_API_KEY": "@pdfshift-api-key"
  }
}